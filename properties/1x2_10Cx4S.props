#
# Servers and clients
#
snd.sim.number_of_servers: 4
snd.sim.client_traffic_exponential: 0.001
snd.sim.number_of_clients: 10
#
# Note that the number of attackers must be lte the number
# of clients, because the attackers are clients.
#
snd.sim.number_of_attackers: 4
#
# same with number of victims and number of servers.
#
snd.sim.number_of_victims: 2
#
snd.sim.attack_probability: 0.01

snd.sim.end_time: 2000000

snd.properties_file: properties/1x2.props
